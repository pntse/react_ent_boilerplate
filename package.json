{
  "name": "rlean_boilerplate",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reach/router": "^1.2.1",
    "@rlean/core": "^0.39.6",
    "@rlean/utils": "^0.1.1",
    "core-js": "^3.3.5",
    "npm-run-all": "^4.1.5",
    "react": "^16.10.0",
    "react-dom": "^16.10.0",
    "react-scripts": "3.1.2",
    "regenerator-runtime": "^0.13.3"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "cross-env REACT_APP_ENV=local npm-run-all -p watch-css start-js",
    "build-js": "react-scripts build && copy web.config build",
    "build": "npm-run-all build-css build-js",
    "build:dev": "cross-env REACT_APP_ENV=dev npm run build",
    "build:staging": "cross-env REACT_APP_ENV=stg npm run build",
    "build:prod": "cross-env REACT_APP_ENV=prod npm run build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "cross-env": "^6.0.0",
    "node-sass-chokidar": "^1.3.5",
    "react-test-renderer": "^16.11.0",
    "wepack": "0.0.1"
  }
}
